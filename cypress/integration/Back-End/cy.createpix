



describe('Create Pix Order', () => {
    it('Deverá realizar a criação do Pix Order com sucesso', () => {
      cy.request({
        method: 'POST',
        url: 'https://partnerhuborderserviceqa.azurewebsites.net/api/Order/CreatePixOrder'
        
      }).then((response) => {
        expect(response.status).to.equal(201);
        expect(response.body.usuarios[0].nome).to.equal('Fulano da Silva')
        expect(response.body.usuarios[0].email).to.equal('fulano@qa.com')
      })
    })
  })